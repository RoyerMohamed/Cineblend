{"ast":null,"code":"import MoviesListUtils from \"./MoviesListUtils.vue\";\nimport SortBoutonUtils from \"./SortBoutonUtils.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"TopMovies\",\n  components: {\n    MoviesListUtils,\n    SortBoutonUtils\n  },\n\n  data() {\n    return {\n      movies: []\n    };\n  },\n\n  created() {\n    console.log(this.$store.getters);\n    axios.get(\"https://api.themoviedb.org/3/discover/movie?api_key=7d83cf4dcf03db332755aef99b57ffe7&language=fr-FR&sort_by=popularity.desc&page=1&vote_count.gte=1000\").then(res => {\n      this.movies = res.data.results;\n      axios.get(\"https://api.themoviedb.org/3/discover/movie?api_key=7d83cf4dcf03db332755aef99b57ffe7&language=fr-FR&sort_by=popularity.desc&page=2&vote_count.gte=1000\").then(res => {\n        res.data.results.forEach(movie => {\n          this.movies.push(movie);\n        });\n      });\n    });\n  },\n\n  methods: {\n    sortedMovies(sortedMovies) {\n      this.movies = sortedMovies;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,eAAP,MAA4B,uBAA5B;AACA,OAAOC,eAAP,MAA4B,uBAA5B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IACVJ,eADU;IAEVC;EAFU,CAFC;;EAObI,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE;IADH,CAAP;EAGD,CAXY;;EAabC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,KAAKC,MAAL,CAAYC,OAAxB;IACAT,KAAI,CACDU,GADH,CAEI,wJAFJ,EAIGC,IAJH,CAISC,GAAD,IAAS;MACb,KAAKR,MAAL,GAAcQ,GAAG,CAACT,IAAJ,CAASU,OAAvB;MACAb,KAAK,CAACU,GAAN,CAAW,wJAAX,EACCC,IADD,CACMC,GAAG,IAAE;QACTA,GAAG,CAACT,IAAJ,CAASU,OAAT,CAAiBC,OAAjB,CAA0BC,KAAI,IAAK;UACjC,KAAKX,MAAL,CAAYY,IAAZ,CAAiBD,KAAjB;QACD,CAFD;MAGD,CALD;IAMD,CAZH;EAaD,CA5BY;;EA4BXE,OAAM,EAAI;IACVC,YAAY,CAACA,YAAD,EAAc;MACxB,KAAKd,MAAL,GAAcc,YAAd;IACD;;EAHS;AA5BC,CAAf","names":["MoviesListUtils","SortBoutonUtils","axios","name","components","data","movies","created","console","log","$store","getters","get","then","res","results","forEach","movie","push","methods","sortedMovies"],"sourceRoot":"","sources":["/Users