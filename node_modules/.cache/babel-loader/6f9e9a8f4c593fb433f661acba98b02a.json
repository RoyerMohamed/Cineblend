{"ast":null,"code":"export default {\n  name: \"sortBoutonUtils\",\n  props: [\"movies\"],\n\n  data() {\n    return {\n      triesParTitre: false,\n      triesParNote: false,\n      triesParDate: false\n    };\n  },\n\n  methods: {\n    triParTitre() {\n      let sorted = this.movies;\n\n      if (!this.triesParTitre) {\n        sorted.sort(function (a, b) {\n          if (a.title < b.title) {\n            return -1;\n          }\n\n          if (a.title > b.title) {\n            return 1;\n          }\n\n          return 0;\n        });\n      } else {\n        sorted.sort(function (a, b) {\n          if (a.title > b.title) {\n            return -1;\n          }\n\n          if (a.title < b.title) {\n            return 1;\n          }\n\n          return 0;\n        });\n      }\n\n      this.triesParTitre = !this.triesParTitre;\n      console.log(sorted);\n    },\n\n    triParNote() {\n      let sorted = this.movies;\n\n      if (!this.triesParNote) {\n        sorted.sort(function (a, b) {\n          if (a.vote_average < b.vote_average) {\n            return -1;\n          }\n\n          if (a.vote_average > b.vote_average) {\n            return 1;\n          }\n\n          return 0;\n        });\n      } else {\n        sorted.sort(function (a, b) {\n          if (a.vote_average > b.vote_average) {\n            return -1;\n          }\n\n          if (a.vote_average < b.vote_average) {\n            return 1;\n          }\n\n          return 0;\n        });\n      }\n\n      this.triesParNote = !this.triesParNote;\n    },\n\n    triParDate() {\n      let sorted = this.movies;\n\n      if (!this.triesParDate) {\n        sorted.sort(function (a, b) {\n          if (a.release_date < b.release_date) {\n            return -1;\n          }\n\n          if (a.release_date > b.release_date) {\n            return 1;\n          }\n\n          return 0;\n        });\n      } else {\n        sorted.sort(function (a, b) {\n          if (a.release_date > b.release_date) {\n            return -1;\n          }\n\n          if (a.release_date < b.release_date) {\n            return 1;\n          }\n\n          return 0;\n        });\n      }\n\n      this.triesParDate = !this.triesParDate;\n      console.log(sorted);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,eAAe;EACbA,IAAI,EAAE,iBADO;EAEbC,KAAK,EAAE,CAAC,QAAD,CAFM;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,YAAY,EAAE,KAFT;MAGLC,YAAY,EAAE;IAHT,CAAP;EAKD,CATY;;EAUbC,OAAO,EAAE;IACPC,WAAW,GAAG;MACV,IAAIC,MAAK,GAAI,KAAKC,MAAlB;;MACA,IAAG,CAAC,KAAKN,aAAT,EAAuB;QACnBK,MAAM,CAACE,IAAP,CAAY,UAASC,CAAT,EAAWC,CAAX,EAAa;UAClB,IAAGD,CAAC,CAACE,KAAF,GAAUD,CAAC,CAACC,KAAf,EAAqB;YACjB,OAAO,CAAC,CAAR;UACJ;;UACA,IAAGF,CAAC,CAACE,KAAF,GAAUD,CAAC,CAACC,KAAf,EAAqB;YACjB,OAAO,CAAP;UACJ;;UACA,OAAO,CAAP;QACD,CARN;MASH,CAVD,MAUK;QACAL,MAAM,CAACE,IAAP,CAAY,UAASC,CAAT,EAAWC,CAAX,EAAa;UACnB,IAAGD,CAAC,CAACE,KAAF,GAAUD,CAAC,CAACC,KAAf,EAAqB;YACjB,OAAO,CAAC,CAAR;UACJ;;UACA,IAAGF,CAAC,CAACE,KAAF,GAAUD,CAAC,CAACC,KAAf,EAAqB;YACjB,OAAO,CAAP;UACJ;;UACA,OAAO,CAAP;QACD,CARL;MAUL;;MACA,KAAKV,aAAL,GAAqB,CAAC,KAAKA,aAA3B;MACAW,OAAO,CAACC,GAAR,CAAYP,MAAZ;IACH,CA3BM;;IA4BPQ,UAAU,GAAG;MACT,IAAIR,MAAK,GAAI,KAAKC,MAAlB;;MACA,IAAG,CAAC,KAAKL,YAAT,EAAsB;QAClBI,MAAM,CAACE,IAAP,CAAY,UAASC,CAAT,EAAWC,CAAX,EAAa;UAClB,IAAGD,CAAC,CAACM,YAAF,GAAiBL,CAAC,CAACK,YAAtB,EAAmC;YAC/B,OAAO,CAAC,CAAR;UACJ;;UACA,IAAGN,CAAC,CAACM,YAAF,GAAiBL,CAAC,CAACK,YAAtB,EAAmC;YAC/B,OAAO,CAAP;UACJ;;UACA,OAAO,CAAP;QACD,CARN;MASH,CAVD,MAUK;QACAT,MAAM,CAACE,IAAP,CAAY,UAASC,CAAT,EAAWC,CAAX,EAAa;UACnB,IAAGD,CAAC,CAACM,YAAF,GAAiBL,CAAC,CAACK,YAAtB,EAAmC;YAC/B,OAAO,CAAC,CAAR;UACJ;;UACA,IAAGN,CAAC,CAACM,YAAF,GAAiBL,CAAC,CAACK,YAAtB,EAAmC;YAC/B,OAAO,CAAP;UACJ;;UACA,OAAO,CAAP;QACD,CARL;MAUL;;MACA,KAAKb,YAAL,GAAoB,CAAC,KAAKA,YAA1B;IACH,CArDM;;IAsDPc,UAAU,GAAG;MACP,IAAIV,MAAK,GAAI,KAAKC,MAAlB;;MACF,IAAG,CAAC,KAAKJ,YAAT,EAAsB;QAClBG,MAAM,CAACE,IAAP,CAAY,UAASC,CAAT,EAAWC,CAAX,EAAa;UAClB,IAAGD,CAAC,CAACQ,YAAF,GAAiBP,CAAC,CAACO,YAAtB,EAAmC;YAC/B,OAAO,CAAC,CA